{
	"schemaVersion": 1,
	"id": "survival-spectator",
	"version": "${version}",
	"name": "Survival Spectator",
	"description": "Simple mod that adds /c command to toggle spectator in survival",
	"authors": [
		"VanJackal"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "GPL-3.0",
	"icon": "assets/survival-spectator/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.njackal.SurvivalSpectator"
		],
		"client": [
			"com.njackal.ExampleModClient"
		],
		"cardinal-components": [
			"com.njackal.persistence.ComponentInit"
		]
	},
	"mixins": [
		"modid.mixins.json",
		{
			"config": "modid.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "1.21.10",
		"java": ">=21",
		"fabric-api": "*",
		"cardinal-components-base": ">=7.0.0-beta.1",
		"cardinal-components-entity": ">=7.0.0-beta.1"
	},
	"suggests": {
	},
	"custom": {
		"cardinal-components" : [
			"survival-spectator:player_spectator"
		]
	}
}